public class People {
	String name;
	int Number;
	String memo;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name=name;
	}
	public int getNumber() {
		return Number;
	}
	public void setNumber(int Number) {
		this.Number=Number;
	}
	public String getmemo() {
		return memo;
	}
	public void setmemo(String memo) {
		this.memo=memo;
	}
	public void Correct(String name,int Number,String memo) {
		this.name=name;
		this.Number=Number;
		this.memo=memo;
	}

}
